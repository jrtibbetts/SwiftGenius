                                    "tag":"a",
                                    "attributes":{
                                        "href":"https://genius.com/David-guetta-titanium-lyrics"
                                    },
                                    "data":{
                                        "api_path":"/songs/64869"
                                    },
                                    "children":[
                                        "Titanium"
                                    ]
                                },
                                "” with ",
                                {
                                    "tag":"a",
                                    "attributes":{
                                        "href":"https://genius.com/artists/David-guetta"
                                    },
                                    "data":{
                                        "api_path":"/artists/2578"
                                    },
                                    "children":[
                                        "David Guetta"
                                    ]
                                },
                                ", “",
                                {
                                    "tag":"a",
                                    "attributes":{
                                        "href":"https://genius.com/Rihanna-diamonds-lyrics"
                                    },
                                    "data":{
                                        "api_path":"/songs/89794"
                                    },
                                    "children":[
                                        "Diamonds"
                                    ]
                                },
                                "” with ",
                                {
                                    "tag":"a",
                                    "attributes":{
                                        "href":"https://genius.com/artists/Rihanna"
                                    },
                                    "data":{
                                        "api_path":"/artists/89"
