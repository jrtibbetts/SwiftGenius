                                    "children":[
                                        "Rihanna"
                                    ]
                                },
                                " and “",
                                {
                                    "tag":"a",
                                    "attributes":{
                                        "href":"https://genius.com/Flo-rida-wild-ones-lyrics"
                                    },
                                    "data":{
                                        "api_path":"/songs/68266"
                                    },
                                    "children":[
                                        "Wild Ones"
                                    ]
                                },
                                "” with ",
                                {
                                    "tag":"a",
                                    "attributes":{
                                        "href":"https://genius.com/artists/Flo-rida"
                                    },
                                    "data":{
                                        "api_path":"/artists/1081"
